{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    { "source": "/assets/:path*", "destination": "/assets/:path*" },
    { "source": "/:path*", "destination": "/api/server" }
  ],
  "routes": [
    {
      "src": "/api/server",
      "dest": "/api/server"
    }
  ]
}